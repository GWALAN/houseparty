{
  "expo": {
    "name": "HouseParty",
    "slug": "houseparty-score-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "houseparty",
    "userInterfaceStyle": "dark",
    "description": "Score everything, anywhere. Track games, compete with friends, and unlock achievements in your custom houses.",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F172A"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "updates": {
      "enabled": false
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.houseparty.scoretracker",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "NSCameraUsageDescription": "HouseParty needs camera access to scan QR codes for joining houses.",
        "NSPhotoLibraryUsageDescription": "HouseParty needs photo library access to set custom profile pictures.",
        "NSPhotoLibraryAddUsageDescription": "HouseParty needs permission to save QR codes to your photo library.",
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": ["houseparty"]
          }
        ]
      },
      "associatedDomains": [
        "applinks:houseparty.app",
        "applinks:*.houseparty.app"
      ]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#0F172A"
      },
      "package": "com.houseparty.scoretracker",
      "versionCode": 1,
      "androidStatusBar": {
        "backgroundColor": "transparent",
        "translucent": true,
        "barStyle": "light-content"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "houseparty.app",
              "pathPrefix": "/invite"
            },
            {
              "scheme": "https",
              "host": "houseparty.app",
              "pathPrefix": "/house"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        },
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "houseparty",
              "host": "paypal"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow HouseParty to scan QR codes to join houses."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow HouseParty to access your photos to set your profile picture.",
          "cameraPermission": "Allow HouseParty to take photos for your profile picture."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#10B981",
          "sounds": []
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "b264b4c4-76ac-4557-a272-db5f97cad31d"
      },
      "router": {}
    },
    "owner": "elandre"
  }
}
